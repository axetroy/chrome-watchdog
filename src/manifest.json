{
  "name": "Watch Dog",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "page_action": {
    "default_title": "Title",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.bundle.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "parser.bundle.js"
  ],
  "permissions": [
    "tabs",
    "webNavigation",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ]
}
